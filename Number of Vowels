words = input("Please enter a string to be evaluated. ")

def countVowels(y):
  numVowels = 0
  for x in y:
    if "a" in y:
      numVowels = numVowels + 1
    elif "e" in y:
      numVowels = numVowels + 1
    elif "i" in y:
      numVowels = numVowels + 1
    elif "o" in y:
      numVowels = numVowels + 1
    elif "u" in y:
      numVowels = numVowels + 1
    elif "y" in y:
      numVowels = numVowels + 1
  return numVowels
      
if hasVowels(words):
  print ("The string " + words + " has at least one vowel.")
else:
  print ("The string " + words + " does not have any vowels.")
